{
  "name": "yuthub-housing-platform",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "tsc && vite build",
    "start": "tsx server/index.ts",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate:db": "node scripts/validate-database-structure.js",
    "validate:security": "node scripts/validate-database-security.js",
    "audit:platform": "npx tsx scripts/audit-platform-admin-db.ts",
    "setup:oauth": "node scripts/configure-oauth.js",
    "populate:councils": "npx tsx scripts/populate-uk-councils.ts",
    "seed:admin": "npx tsx scripts/seed-platform-admin.ts",
    "deploy": "npm run build && npm start",
    "prepare": "husky install"
  },
  "dependencies": {
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "vite": "^6.3.5"
  }
